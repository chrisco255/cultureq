{
  "name": "culture-shock-server",
  "version": "1.0.0",
  "description": "Culture Shock",
  "main": "src/index.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "scripts": {
    "test": "node_modules/.bin/mocha \"src/**/*.spec.js\" --compilers js:babel-core/register --require test/config.js",
    "test:watch": "npm run test -- --watch",
    "dev": "webpack-dev-server",
    "build": "webpack -p --config webpack.production.config.js",
    "prod": "npm run build && http-server ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/ultilabs/culture-shock-client.git"
  },
  "author": "http://ultilabs.xyz",
  "license": "ISC",
  "homepage": "https://bitbucket.org/ultilabs/culture-shock-client#readme",
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.9.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "http-server": "^0.9.0",
    "mocha": "^2.5.3",
    "react-addons-test-utils": "^15.0.2",
    "react-css-modules": "^3.7.6",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "email-regex": "^1.0.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-form": "^5.2.3",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.10.4"
  }
}
