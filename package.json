{
  "name": "culture-shock-server",
  "version": "1.0.0",
  "description": "Culture Shock",
  "main": "src/index.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "scripts": {
    "test": "mocha \"src/**/*.spec.js\" --compilers js:babel-core/register --require test/config.js",
    "test:watch": "npm run test -- --watch",
    "start:dev": "cross-env NODE_ENV=development node server.js",
    "start": "npm run start:dev",
    "build": "rimraf build && cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "start:prod": "cross-env NODE_ENV=production node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/ultilabs/culture-shock-client.git"
  },
  "author": "http://ultilabs.xyz",
  "license": "ISC",
  "homepage": "https://bitbucket.org/ultilabs/culture-shock-client#readme",
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "babel-core": "6.10.4",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "6.10.2",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "chai": "3.5.0",
    "clean-webpack-plugin": "0.1.9",
    "cross-env": "^1.0.8",
    "css-loader": "0.23.1",
    "css-modules-require-hook": "4.0.1",
    "enzyme": "2.3.0",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.21.0",
    "http-server": "0.9.0",
    "json-loader": "^0.5.4",
    "mocha": "2.5.3",
    "react-addons-test-utils": "15.1.0",
    "react-css-modules": "3.7.6",
    "react-hot-loader": "1.3.0",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "rimraf": "^2.5.2",
    "stats-webpack-plugin": "^0.3.1",
    "style-loader": "0.13.1",
    "transform-loader": "^0.2.3",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-validator": "2.2.0"
  },
  "dependencies": {
    "auth0-lock": "^9.2.1",
    "axios": "^0.12.0",
    "email-regex": "1.0.0",
    "express": "^4.14.0",
    "http-proxy": "^1.14.0",
    "lodash": "^4.13.1",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.4.1",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2",
    "redux-form": "5.2.5",
    "redux-saga": "0.10.5",
    "socket.io-client": "^1.4.6",
    "validations": "bitbucket:ultilabs/validations#HEAD"
  }
}
