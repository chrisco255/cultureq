{
  "name": "culture-shock-server",
  "version": "1.0.0",
  "description": "Culture Shock",
  "main": "src/index.js",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "scripts": {
    "test": "mocha \"src/**/*.spec.js\" --compilers js:babel-core/register --require test/config.js",
    "test:watch": "npm run test -- --watch",
    "start:dev": "cross-env NODE_ENV=development node server.js",
    "start": "npm run start:dev",
    "build": "rimraf build && cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "start:prod": "cross-env NODE_ENV=production node server.js",
    "lint": "eslint src/**"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/ultilabs/culture-shock-client.git"
  },
  "author": "http://ultilabs.xyz",
  "license": "ISC",
  "homepage": "https://bitbucket.org/ultilabs/culture-shock-client#readme",
  "devDependencies": {
    "autoprefixer-loader": "3.2.0",
    "babel-core": "6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-class-properties": "6.11.5",
    "babel-plugin-transform-object-rest-spread": "6.8.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.11.1",
    "chai": "3.5.0",
    "clean-webpack-plugin": "0.1.10",
    "cross-env": "2.0.0",
    "css-loader": "0.23.1",
    "css-modules-require-hook": "4.0.1",
    "enzyme": "2.4.1",
    "eslint": "^3.1.1",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.22.0",
    "json-loader": "^0.5.4",
    "mocha": "2.5.3",
    "react-addons-test-utils": "15.2.1",
    "react-css-modules": "3.7.8",
    "react-hot-loader": "1.3.0",
    "redux-devtools": "3.3.1",
    "redux-devtools-dock-monitor": "1.1.1",
    "redux-devtools-log-monitor": "1.0.11",
    "rimraf": "2.5.2",
    "stats-webpack-plugin": "0.4.0",
    "style-loader": "0.13.1",
    "transform-loader": "0.2.3",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "2.12.2",
    "webpack-validator": "2.2.6"
  },
  "dependencies": {
    "auth0-lock": "9.2.2",
    "axios": "0.13.1",
    "email-regex": "1.0.0",
    "express": "^4.14.0",
    "http-proxy": "^1.14.0",
    "lodash": "^4.14.0",
    "react": "15.2.1",
    "react-dom": "15.2.1",
    "react-redux": "4.4.5",
    "react-router": "2.6.0",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2",
    "redux-form": "^5.3.1",
    "redux-saga": "0.11.0",
    "rimraf": "^2.5.4",
    "socket.io-client": "^1.4.6",
    "validations": "bitbucket:ultilabs/validations#HEAD"
  }
}
